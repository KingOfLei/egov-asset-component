    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[private RowMetaInterface tmpRowMeta;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();
	if (r == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        //赋值
        data.outputRowMeta = getInputRowMeta().clone();

        tmpRowMeta = getInputRowMeta().clone();
        first = false;
    }
    Object[] outputData = RowDataUtil.allocateRowData(tmpRowMeta.size());
    for (int i = 1,size = tmpRowMeta.size(); i <= size ; i++){
       
         ValueMetaInterface value =  tmpRowMeta.getValueMeta(i-1);
         	value.setType(ValueMetaInterface.TYPE_STRING);
	    	ValueMetaInterface toMeta = data.outputRowMeta.getValueMeta(i-1);
           switch(toMeta.getType()){
	            case ValueMetaInterface.TYPE_STRING    : outputData[i-1] = value.getString(r[i-1]); break;
	            case ValueMetaInterface.TYPE_NUMBER    : outputData[i-1] = value.getNumber(r[i-1]); break;
	            case ValueMetaInterface.TYPE_INTEGER   : outputData[i-1] = value.getInteger(r[i-1]); break;
	            case ValueMetaInterface.TYPE_DATE      : outputData[i-1] = value.getDate(r[i-1]); break;
	            case ValueMetaInterface.TYPE_BIGNUMBER : outputData[i-1] = value.getBigNumber(r[i-1]); break;
	            case ValueMetaInterface.TYPE_BOOLEAN   : outputData[i-1] = value.getBoolean(r[i-1]); break;
	            case ValueMetaInterface.TYPE_BINARY    : outputData[i-1] = value.getBinary(r[i-1]); break;
	            default: throw new KettleValueException("不能转换 '"+value+"' 成类型  "+toMeta.getType());       
	    	} 
    }
   
    putRow(data.outputRowMeta, outputData);

	return true;
}
]]></class_source>
        </definition>
