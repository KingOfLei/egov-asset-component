    r[Index++] = getVariable("SYSTEM_CLASSNAME", "");
    r[Index++] = getVariable("SYSTEM_USERNAME", "");
    r[Index++] = getVariable("SYSTEM_PASSWORD", "");

    putRow(data.outputRowMeta, r);

	return true;
}
