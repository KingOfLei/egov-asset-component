        <class_source><![CDATA[import java.sql.*;
import org.pentaho.di.core.database.*;
import java.util.Arrays;

private DatabaseMeta databaseMeta;
private Database database;
private String tableName = null;
private String sql = null;
private RowMetaInterface tableMeta = null;
private RowMetaInterface copyMeta = null;
private RowMetaInterface outMeta = null;
private int[] indexNrsTmp,indexNrs;
private int[] indexConverTmp,indexConver;
private int[] newIndex ;
private boolean readInfo;
private boolean IsExcludeField;
private boolean isChange;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    if(readInfo){
        readInfo = false;
        tableName = getTrans().environmentSubstitute("${F_DEST_DATA}");
        sql = "SELECT * FROM " + tableName + " WHERE 1=2";
        database.openQuery(sql);
        tableMeta = database.getReturnRowMeta();//获取字段类型数据
        return true;
    }
    
    Object[] r = getRow();
    if (r == null) {
       setOutputDone();
       return false;
    }
    if (first)
    {
         copyMeta = getInputRowMeta().clone();
         outMeta = getInputRowMeta().clone();
         //大小
         indexNrsTmp = new int[copyMeta.size()]; 
         indexConverTmp = new int[tableMeta.size()];//存储值转换字段索引
         int num = 0;
         int num1 = 0;
         for(int i = 0,CopySize = copyMeta.size() ; i<CopySize ; i++){
            {
                   String Name = copyMeta.getValueMeta(i).getName();
                   int Index = tableMeta.indexOfValue(Name);

                   if (Index == -1){
                       
                        indexNrsTmp[num] = i;//获取需要删除的字段索引
                        outMeta.removeValueMeta(Name);//移除字段
                        num++;
                        continue;
                   }
            }
         }
         //删除对于数组值
         indexNrs = Arrays.copyOf(indexNrsTmp,num);
         first = false;
    }
    r = createOutputRow(r, getInputRowMeta().size());
    Arrays.sort(indexNrs);
    r = RowDataUtil.removeItems(r,indexNrs);//移除目标表中不需要的值
    putRow(outMeta, r);
    return true;
}

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface)
{
    if (!parent.initImpl(stepMetaInterface, stepDataInterface)) {
      return false;
    }
    readInfo = true;
    isChange = true;
    String dbName = "System_data_Native";
    databaseMeta = getTransMeta().findDatabase(dbName);
    if (databaseMeta==null) {
         logError("A connection with name "+dbName+" could not be found!");
         setErrors(1);
         return false;
    }

    database = new Database(getTrans(), databaseMeta);

    try  {
      database.connect();
    } catch(Exception e) {
      logError("Connecting to database "+dbName+" failed.", e);
      setErrors(1);
      return false;
    }

    return true;
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi)
{
    if (database!=null) {
      database.disconnect();
    }

    parent.disposeImpl(smi, sdi);
}


