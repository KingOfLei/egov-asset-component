    <sql>SELECT F_ZC_ID AS F_BILL_ID,1 AS F_SXH,F_START_DATE AS F_ZC_DATE,F_START_DATE AS F_BILL_DATE,&apos;资产新增&apos; AS F_BILL_MEMO,&apos;11&apos; AS F_BILL_TYPE,
F_ORG_ID,F_ORG_CODE,F_ORG_NAME,F_ZC_ID,F_ZC_CODE,F_ZC_NAME,F_ZC_SN,F_ZC_TYPE_CODE,F_CLASSIFY_CODE,F_CLASSIFY_NAME,
F_DEPT_CODE,F_DEPT_NAME,&apos;0&apos; AS F_ZC_STATE,&apos;9&apos; AS F_AUDIT_STATE,F_CREATOR,F_CREATEDATE,
F_ZC_UNIT,F_VALUE_TYPE_CODE,F_VALUE_TYPE_NAME,F_ZC_ISL,F_ZC_CSL,F_ZC_CJE,F_ZC_IJE,F_ZC_PRICE,
F_USE_YEAR,
F_FISCAL_ALLOCATE_FUND,F_CAREER_FUND,F_OUT_BUDGET_FUND,F_OTHER_FUND,F_FISCAL_SURPLUS_FUND,
F_MANAGER_STAFF,F_KEEP_STAFF,F_USE_STAFF,F_USE_WAY_CODE,F_USE_WAY_NAME,F_BUY_DATE,F_ENTRY_DATE,
F_START_DATE,F_USE_STATE_CODE,F_USE_STATE_NAME,F_DEPR_CODE,F_DEPR_NAME,

&apos;图书、档案&apos; AS F_ZC_TYPE_NAME,F_DEPR_CZL,
F_ZC_ZJE,F_ZC_LZJE,
0 AS F_ZC_BJE,F_ZC_IJE AS F_ZC_EJE,F_ZC_IJE AS F_ZC_DJE,0 AS F_ZC_BSL,F_ZC_ISL AS F_ZC_ESL,F_ZC_ISL AS F_ZC_DSL,F_TZ_CODE,F_HSDW_CODE
FROM TZC6_51TS   A   WHERE NOT EXISTS(SELECT 1 FROM VUNION_WASTE_ALLOT BF WHERE BF.F_AUDIT_STATE=9
 AND A.F_ZC_ID=BF.F_ZC_ID AND A.F_ORG_ID=BF.F_ORG_ID AND BF.F_ORG_CODE IN (${ORG_CODE}))
AND  NOT EXISTS(SELECT 1 FROM TZC_INFO B WHERE A.F_ZC_ID=B.F_ZC_ID AND A.F_ORG_ID=B.F_ORG_ID) AND A.F_ORG_CODE IN (${ORG_CODE})

UNION ALL

SELECT A.F_ZC_ID AS F_BILL_ID,1 AS F_SXH,F_START_DATE AS F_ZC_DATE,F_START_DATE AS F_BILL_DATE,&apos;资产新增&apos; AS F_BILL_MEMO,&apos;11&apos; AS F_BILL_TYPE,
A.F_ORG_ID,A.F_ORG_CODE,F_ORG_NAME,A.F_ZC_ID,A.F_ZC_CODE,F_ZC_NAME,F_ZC_SN,A.F_ZC_TYPE_CODE,F_CLASSIFY_CODE,F_CLASSIFY_NAME,
F_DEPT_CODE,F_DEPT_NAME,&apos;0&apos; AS F_ZC_STATE,&apos;9&apos; AS F_AUDIT_STATE,F_CREATOR,F_CREATEDATE,
F_ZC_UNIT,F_VALUE_TYPE_CODE,F_VALUE_TYPE_NAME,F_ZC_ISL,F_ZC_CSL,F_ZC_CJE,F_ZC_IJE,F_ZC_PRICE,
F_USE_YEAR,
F_FISCAL_ALLOCATE_FUND,F_CAREER_FUND,F_OUT_BUDGET_FUND,F_OTHER_FUND,F_FISCAL_SURPLUS_FUND,
F_MANAGER_STAFF,F_KEEP_STAFF,F_USE_STAFF,F_USE_WAY_CODE,F_USE_WAY_NAME,F_BUY_DATE,F_ENTRY_DATE,
F_START_DATE,F_USE_STATE_CODE,F_USE_STATE_NAME,F_DEPR_CODE,F_DEPR_NAME,

&apos;图书、档案&apos; AS F_ZC_TYPE_NAME,F_DEPR_CZL,
F_ZC_ZJE,F_ZC_LZJE,
0 AS F_ZC_BJE,F_ZC_IJE AS F_ZC_EJE,F_ZC_IJE AS F_ZC_DJE,0 AS F_ZC_BSL,F_ZC_ISL AS F_ZC_ESL,F_ZC_ISL AS F_ZC_DSL,F_TZ_CODE,F_HSDW_CODE
FROM TZC6_51TS   A, (SELECT F_ORG_ID,F_ORG_CODE,F_BILL_ID,F_BILL_DATE,F_ZC_TYPE_CODE,F_ZC_ID,F_ZC_CODE FROM
VUNION_WASTE_ALLOT WHERE F_AUDIT_STATE=9 AND F_ORG_CODE IN (${ORG_CODE}) ) BF
WHERE A.F_ZC_ID=BF.F_ZC_ID AND A.F_ORG_ID=BF.F_ORG_ID  AND 9=A.F_AUDIT_STATE AND A.F_ZC_STATE IN (3,5) AND A.F_ORG_CODE IN (${ORG_CODE})

UNION ALL

SELECT BF.F_BILL_ID,2 AS F_SXH,BF.F_BILL_DATE AS F_ZC_DATE,BF.F_BILL_DATE,BF.F_NAME AS F_BILL_MEMO,BF.F_TYPE AS F_BILL_TYPE,
A.F_ORG_ID,A.F_ORG_CODE,A.F_ORG_NAME,A.F_ZC_ID,A.F_ZC_CODE,A.F_ZC_NAME,A.F_ZC_SN,A.F_ZC_TYPE_CODE,A.F_CLASSIFY_CODE,A.F_CLASSIFY_NAME,
A.F_DEPT_CODE,A.F_DEPT_NAME,&apos;3&apos; AS F_ZC_STATE,&apos;9&apos; AS F_AUDIT_STATE,A.F_CREATOR,A.F_CREATEDATE,
A.F_ZC_UNIT,A.F_VALUE_TYPE_CODE,A.F_VALUE_TYPE_NAME,A.F_ZC_ISL,0 AS F_ZC_CSL,0 AS F_ZC_CJE,A.F_ZC_IJE,A.F_ZC_PRICE,
A.F_USE_YEAR,
A.F_FISCAL_ALLOCATE_FUND,A.F_CAREER_FUND,A.F_OUT_BUDGET_FUND,A.F_OTHER_FUND,A.F_FISCAL_SURPLUS_FUND,
A.F_MANAGER_STAFF,A.F_KEEP_STAFF,A.F_USE_STAFF,A.F_USE_WAY_CODE,A.F_USE_WAY_NAME,A.F_BUY_DATE,A.F_ENTRY_DATE,
A.F_START_DATE,A.F_USE_STATE_CODE,A.F_USE_STATE_NAME,A.F_DEPR_CODE,A.F_DEPR_NAME,

&apos;图书、档案&apos; AS F_ZC_TYPE_NAME,A.F_DEPR_CZL,
A.F_ZC_ZJE,A.F_ZC_LZJE,
A.F_ZC_CJE AS F_ZC_BJE,0 AS F_ZC_EJE,-A.F_ZC_CJE AS F_ZC_DJE,A.F_ZC_CSL AS F_ZC_BSL,0 AS F_ZC_ESL,-A.F_ZC_CSL AS F_ZC_DSL,F_TZ_CODE,F_HSDW_CODE
FROM TZC6_51TS A, (SELECT F_TYPE,F_NAME,F_ORG_ID,F_ORG_CODE,F_BILL_ID,F_BILL_DATE,F_ZC_TYPE_CODE,F_ZC_ID,F_ZC_CODE FROM
VUNION_WASTE_ALLOT WHERE F_AUDIT_STATE=9 AND F_ORG_CODE IN (${ORG_CODE})) BF
WHERE A.F_ZC_ID=BF.F_ZC_ID AND A.F_ORG_ID=BF.F_ORG_ID  AND 9=A.F_AUDIT_STATE AND A.F_ZC_STATE IN (3,5) AND A.F_ORG_CODE IN (${ORG_CODE})

UNION ALL

SELECT F_ZC_ID AS F_BILL_ID,1 AS F_SXH,F_START_DATE AS F_ZC_DATE,F_START_DATE AS F_BILL_DATE,&apos;资产新增&apos; AS F_BILL_MEMO,&apos;11&apos; AS F_BILL_TYPE,
F_ORG_ID,F_ORG_CODE,F_ORG_NAME,F_ZC_ID,F_ZC_CODE,F_ZC_NAME,F_ZC_SN,F_ZC_TYPE_CODE,F_CLASSIFY_CODE,F_CLASSIFY_NAME,
F_DEPT_CODE,F_DEPT_NAME,&apos;0&apos; AS F_ZC_STATE,&apos;9&apos; AS F_AUDIT_STATE,F_CREATOR,F_CREATEDATE,
F_ZC_UNIT,F_VALUE_TYPE_CODE,F_VALUE_TYPE_NAME,F_ZC_ISL,F_ZC_CSL,F_ZC_CJE,F_ZC_IJE,F_ZC_PRICE,
F_USE_YEAR,
F_FISCAL_ALLOCATE_FUND,F_CAREER_FUND,F_OUT_BUDGET_FUND,F_OTHER_FUND,F_FISCAL_SURPLUS_FUND,
F_MANAGER_STAFF,F_KEEP_STAFF,F_USE_STAFF,F_USE_WAY_CODE,F_USE_WAY_NAME,F_BUY_DATE,F_ENTRY_DATE,
F_START_DATE,F_USE_STATE_CODE,F_USE_STATE_NAME,F_DEPR_CODE,F_DEPR_NAME,

